{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules"
    ]
  },
  "jsRules": {},
  "rules": {
    "trailing-comma": [ false ],
    "no-namespace": [false],
    "no-internal-module": false,
    "no-console": false
  },
  "rulesDirectory": []
}
